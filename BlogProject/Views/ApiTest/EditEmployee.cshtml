@model BlogProject.Controllers.tempEmployeeClass
@{
    ViewData["Title"] = "EditEmployee";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>Api ile Çalışan Güncelleme Sayfası</h1>

<form method="post">
    <label>ID</label>
    @Html.TextBoxFor(x=>x.ID,new {@class="form-control"})
    <br />
    <label>Personel Adı Soyadı</label>
    @Html.TextBoxFor(x=>x.Name, new {@class="form-method"})
    <br />
    <button class="btn btn-outline-success">Güncelle</button>
</form>